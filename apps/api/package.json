{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "MAOF API Server â€” Fastify + TypeScript + PostgreSQL",
  "main": "dist/server.js",
  "scripts": {
    "dev": "DOTENV_CONFIG_PATH=../../.env tsx watch src/server.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:migrate:test": "MAOF_DB_NAME=maof_test tsx src/db/migrate.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@fastify/cors": "^10.0.0",
    "@fastify/jwt": "^9.0.0",
    "@fastify/websocket": "^11.2.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.0.0",
    "dotenv": "^17.3.1",
    "drizzle-orm": "^0.39.0",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.1.0",
    "ioredis": "^5.0.0",
    "openai": "^6.22.0",
    "pg": "^8.0.0",
    "pino-pretty": "^13.1.3",
    "uuid": "^11.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/pg": "^8.0.0",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^3.0.0",
    "drizzle-kit": "^0.30.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
